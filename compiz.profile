[winrules]
s0_size_matches = class=Gvim;
s0_size_width_values = 790;
s0_size_height_values = 720;

[grid]
s0_put_left_key = <Control><Alt>h
s0_put_right_key = <Control><Alt>l
s0_put_top_key = <Control><Alt>k
s0_put_bottom_key = <Control><Alt>j
s0_put_maximize_key = <Control><Alt>m

[core]
s0_active_plugins = core;bailer;detection;composite;opengl;decor;compiztoolbox;gnomecompat;imgpng;resize;regex;snap;winrules;place;grid;animation;resizeinfo;mousepoll;workarounds;move;vpswitch;staticswitcher;expo;wall;session;fade;scale;unityshell;
s0_focus_prevention_level = 2
s0_hsize = 3

[gnomecompat]
s0_command_terminal = xterm
s0_run_command_terminal_key = <Mod4>x

